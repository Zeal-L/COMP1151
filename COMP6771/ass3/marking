   281  TEST_CASE("erase_node - single node") {
   282          auto pip = ppl::pipeline{};
   283          const auto source = pip.create_node<source_produce_num<int, 0, 10>>();
   284          pip.erase_node(source);
   285          CHECK(pip.get_node(source) == nullptr);
   286  }
   287
   288  TEST_CASE("erase_node - multiple nodes with connections") {
   289          auto pip = ppl::pipeline{};
   290          const auto source_a = pip.create_node<source_produce_num<int, 0, 10>>();
   291          const auto source_b = pip.create_node<source_produce_num<int, 10, 0>>();
   292          const auto component = pip.create_node<component_add<int, int, int>>();
   293          const auto sink = pip.create_node<sink_print<int>>();
   294
   295          pip.connect(source_a, component, 0);
   296          pip.connect(source_b, component, 1);
   297          pip.connect(component, sink, 0);
   298
   299          CHECK_FALSE(pip.get_node(component) == nullptr);
   300          CHECK_FALSE(pip.get_dependencies(source_a).empty());
   301          CHECK_FALSE(pip.get_dependencies(source_b).empty());
   302
   303          pip.erase_node(component);
   304
   305          CHECK(pip.get_node(component) == nullptr);
   306          CHECK(pip.get_dependencies(source_a).empty());
   307          CHECK(pip.get_dependencies(source_b).empty());
   308  }
   309
