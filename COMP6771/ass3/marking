   541
   542  TEST_CASE("is_valid - There are no cycles.") {
   543          SECTION("Self connect - source == dest") {
   544                  auto pip = ppl::pipeline{};
   545                  const auto source = pip.create_node<source_produce_num<int, 0, 10>>();
   546                  const auto component = pip.create_node<component_add<int, int, int>>();
   547                  pip.connect(source, component, 0);
   548                  pip.connect(component, component, 1);
   549
   550                  CHECK_FALSE(pip.is_valid());
   551          }
   552
   553          SECTION("big cycle") {
   554                  auto pip = ppl::pipeline{};
   555                  const auto source = pip.create_node<source_produce_num<int, 0, 10>>();
   556                  const auto component1 = pip.create_node<component_add<int, int, int>>();
   557                  const auto component2 = pip.create_node<component_double_it<int>>();
   558                  const auto component3 = pip.create_node<component_double_it<int>>();
   559                  const auto sink = pip.create_node<sink_print<int>>();
   560
   561                  pip.connect(source, component2, 0);
   562                  pip.connect(component2, component3, 0);
   563                  pip.connect(component3, component1, 0);
   564                  pip.connect(component1, component1, 1);
   565                  pip.connect(component1, sink, 0);
   566
   567                  CHECK_FALSE(pip.is_valid());
   568          }
   569  }
   570
   571  TEST_CASE("step - simple successful") {
   572          std::string line;
   573          std::stringstream buffer;
   574          std::streambuf* old = std::cout.rdbuf(buffer.rdbuf());
   575
   576          auto pip = ppl::pipeline{};
   577          const auto source = pip.create_node<source_produce_num<int, 0, 10>>();
   578          const auto sink = pip.create_node<sink_print<int>>();
   579
   580          pip.connect(source, sink, 0);
   581
   582          CHECK(pip.is_valid());
   583          CHECK_FALSE(pip.step());
   584          CHECK_FALSE(pip.step());
   585          CHECK_FALSE(pip.step());
   586
   587          int count = 0;
   588          while (std::getline(buffer, line)) {
   589                  CHECK(line == std::to_string(++count));
   590          }
   591
   592          std::cout.rdbuf(old);
   593  }
   594
   595  TEST_CASE("step - If the node is closed, close all nodes that depend on it.") {
   596          std::string line;
   597          std::stringstream buffer;
   598          std::streambuf* old = std::cout.rdbuf(buffer.rdbuf());
   599
   600          auto pip = ppl::pipeline{};
   601          const auto source = pip.create_node<source_produce_num<int, 0, 1>>();
   602          const auto component1 = pip.create_node<component_double_it<int>>();
   603          const auto component2 = pip.create_node<component_double_it<int>>();
   604          const auto sink = pip.create_node<sink_print<int>>();
   605
   606          pip.connect(source, component1, 0);
   607          pip.connect(component1, component2, 0);
   608          pip.connect(component2, sink, 0);
   609
   610          CHECK(pip.is_valid());
   611          CHECK_FALSE(pip.step());
   612          CHECK_FALSE(pip.step());
   613          CHECK(pip.step());
   614
   615          int count = 1;
   616          while (std::getline(buffer, line)) {
   617                  CHECK(line == std::to_string(count++ * 2 * 2));
   618          }
   619
   620          std::cout.rdbuf(old);
   621  }
   622
   623  TEST_CASE("step - If the node has no value, skip all nodes that depend on it.") {
   624          std::stringstream buffer;
   625          std::streambuf* old = std::cout.rdbuf(buffer.rdbuf());
   626
   627          auto pip = ppl::pipeline{};
   628          const auto source = pip.create_node<source_produce_num<int, 0, 10>>();
   629          const auto component1 = pip.create_node<component_double_it_but_skip_odd<int>>();
   630          const auto component2 = pip.create_node<component_double_it<int>>();
   631          const auto component3 = pip.create_node<component_double_it<int>>();
   632          const auto sink = pip.create_node<sink_print<int>>();
   633
   634          pip.connect(source, component1, 0);
   635          pip.connect(component1, component2, 0);
   636          pip.connect(component2, component3, 0);
   637          pip.connect(component3, sink, 0);
   638
   639          CHECK(pip.is_valid());
   640          CHECK_FALSE(pip.step());
   641          CHECK_FALSE(pip.step());
   642          CHECK_FALSE(pip.step());
   643
   644          CHECK(buffer.str() == "16\n");
   645
   646          std::cout.rdbuf(old);
   647  }
   648
   649  TEST_CASE("step - empty case - polling a node only if all its dependencies are ready.") {
   650          std::stringstream buffer;
   651          std::streambuf* old = std::cout.rdbuf(buffer.rdbuf());
   652
   653          auto pip = ppl::pipeline{};
   654          const auto source1 = pip.create_node<source_produce_num<int, 0, 10>>();
   655          const auto source2 = pip.create_node<source_produce_num<int, 0, 10>>();
   656          const auto component1 = pip.create_node<component_double_it_but_skip_odd<int>>();
   657          const auto component2 = pip.create_node<component_add<int, int, int>>();
   658          const auto sink = pip.create_node<sink_print<int>>();
   659
   660          pip.connect(source1, component1, 0);
   661          pip.connect(component1, component2, 0);
   662          pip.connect(source2, component2, 1);
   663          pip.connect(component2, sink, 0);
   664
   665          CHECK(pip.is_valid());
   666          CHECK_FALSE(pip.step());
   667          CHECK_FALSE(pip.step());
   668          CHECK_FALSE(pip.step());
   669
   670          CHECK(buffer.str() == "6\n");
   671
   672          std::cout.rdbuf(old);
   673  }
   674
   675  TEST_CASE("run - one source, one sink") {
   676          std::string line;
   677          std::stringstream buffer;
   678          std::streambuf* old = std::cout.rdbuf(buffer.rdbuf());
   679
   680          auto pip = ppl::pipeline{};
   681          const auto source = pip.create_node<source_produce_num<int, 0, 10>>();
   682          const auto sink = pip.create_node<sink_print<int>>();
   683
   684          pip.connect(source, sink, 0);
   685
   686          CHECK(pip.is_valid());
   687          pip.run();
   688
   689          int count = 0;
   690          while (std::getline(buffer, line)) {
   691                  CHECK(line == std::to_string(++count));
   692          }
   693          CHECK(count == 11);
   694
   695          std::cout.rdbuf(old);
   696  }
   697
   698  TEST_CASE("run - four source, six component, three sink") {
   699          std::string line;
   700          std::stringstream buffer;
   701          std::streambuf* old = std::cout.rdbuf(buffer.rdbuf());
   702
   703          auto pip = ppl::pipeline{};
   704          const auto source_a = pip.create_node<source_produce_num<int, 0, 100>>();
   705          const auto source_b = pip.create_node<source_produce_num<int, 0, 100>>();
   706          const auto source_c = pip.create_node<source_produce_num<std::size_t, 0, 100>>();
   707          const auto source_d = pip.create_node<source_produce_num<std::size_t, 0, 100>>();
   708
   709          const auto component_lv1_a = pip.create_node<component_add<int, int, std::size_t>>();
   710          const auto component_lv1_b = pip.create_node<component_add<int, std::size_t, std::size_t>>();
   711          const auto component_lv1_c = pip.create_node<component_add<std::size_t, std::size_t, std::size_t>>();
   712          const auto component_lv2_a = pip.create_node<component_add<std::size_t, std::size_t, std::size_t>>();
   713          const auto component_lv2_b = pip.create_node<component_add<std::size_t, std::size_t, std::size_t>>();
   714          const auto component_lv3 = pip.create_node<component_add_but_skip_odd<std::size_t, std::size_t, std::size_t>>();
   715
   716          const auto sink_a = pip.create_node<sink_print<std::size_t>>();
   717          const auto sink_b = pip.create_node<sink_print<std::size_t>>();
   718          const auto sink_c = pip.create_node<sink_print<std::size_t>>();
   719
   720          pip.connect(source_a, component_lv1_a, 0);
   721          pip.connect(source_b, component_lv1_a, 1);
   722          pip.connect(source_b, component_lv1_b, 0);
   723          pip.connect(source_c, component_lv1_b, 1);
   724          pip.connect(source_c, component_lv1_c, 0);
   725          pip.connect(source_d, component_lv1_c, 1);
   726
   727          pip.connect(component_lv1_a, component_lv2_a, 0);
   728          pip.connect(component_lv1_b, component_lv2_a, 1);
   729          pip.connect(component_lv1_b, component_lv2_b, 0);
   730          pip.connect(component_lv1_c, component_lv2_b, 1);
   731
   732          pip.connect(component_lv2_a, component_lv3, 0);
   733          pip.connect(component_lv2_b, component_lv3, 1);
   734
   735          pip.connect(component_lv2_a, sink_a, 0);
   736          pip.connect(component_lv3, sink_b, 0);
   737          pip.connect(component_lv2_b, sink_c, 0);
   738
   739          CHECK(pip.is_valid());
   740          pip.run();
   741
   742          for (int i = 1; i < 102; ++i) {
   743                  std::getline(buffer, line);
   744                  CHECK(line == std::to_string(i + i + i + i));
   745                  std::getline(buffer, line);
   746                  CHECK(line == std::to_string(i + i + i + i));
   747                  std::getline(buffer, line);
   748                  CHECK(line == std::to_string((i + i + i + i) * 2));
   749          }
   750
   751          std::getline(buffer, line);
   752          CHECK(line.size() == 0);
   753
   754          std::cout.rdbuf(old);
   755  }
   756
