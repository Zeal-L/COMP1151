   317  TEST_CASE("erase_node - node is no longer a valid handle after it is erased") {
   318          auto pip = ppl::pipeline{};
   319          const auto source = pip.create_node<source_produce_num<int, 0, 10>>();
   320          pip.erase_node(source);
   321          const auto source2 = pip.create_node<source_produce_num<int, 0, 10>>();
   322
   323          CHECK(source != source2);
   324          CHECK(source2 == 2);
   325  }
