    19  namespace ppl {
    20
    21          // Errors that may occur in a pipeline.
    22          enum class pipeline_error_kind {
    23                  // An expired node ID was provided.
    24                  invalid_node_id,
    25                  // Attempting to bind a non-existant slot.
    26                  no_such_slot,
    27                  // Attempting to bind to a slot that is already filled.
    28                  slot_already_used,
    29                  // The output type and input types for a connection don't match.
    30                  connection_type_mismatch,
    31          };
    32
    33          struct pipeline_error : std::exception {
    34                  explicit pipeline_error(pipeline_error_kind kind) noexcept
    35                  : kind_(kind) {}
    36                  auto kind() const noexcept -> pipeline_error_kind {
    37                          return kind_;
    38                  }
    39                  auto what() const noexcept -> const char* override {
    40                          switch (kind_) {
    41                          case pipeline_error_kind::invalid_node_id: return "invalid node ID";
    42                          case pipeline_error_kind::no_such_slot: return "no such slot";
    43                          case pipeline_error_kind::slot_already_used: return "slot already used";
    44                          case pipeline_error_kind::connection_type_mismatch: return "connection type mismatch";
    45                          }
    46                          return "Unknown pipeline error";
    47                  }
    48
    49           private:
    50                  pipeline_error_kind kind_;
    51          };
    52
    53          // The result of a poll_next() operation.
    54          enum class poll {
    55                  // A value is available.
    56                  ready,
    57                  // No value is available this time, but there might be one later.
    58                  empty,
    59                  // No value is available, and there never will be again:
    60                  // every future poll for this node will return `poll::closed` again.
    61                  closed,
    62          };
    63
    64          // a node generalises producer<O> over "any component at all".
    65          class node {
    66           public:
    67                  virtual auto name() const -> std::string = 0;
    68                  virtual ~node() = default;
    69
    70           private:
    71                  virtual auto poll_next() -> poll = 0;
    72                  // Preconditions: slot is a valid index, and source is either a pointer
    73                  // to a producer of the correct type, or nullptr.
    74                  virtual void connect(const node* source, int slot) = 0;
    75
    76                  // You may add any other virtual functions you feel you may want here.
    77                  poll poll_state = poll::ready;
    78                  auto set_state(poll state) noexcept -> void {
    79                          poll_state = state;
    80                  }
    81
    82                  auto get_state() const noexcept -> poll {
    83                          return poll_state;
    84                  }
    85
    86                  friend class pipeline;
    87          };
    88
    89          // A producer<O> generalises component<I, O> over "any component producing Os"
    90          template<typename Output>
    91          struct producer : node {
    92                  using output_type = Output;
    93                  // only when `Output` is not `void`
    94                  // Preconditions: poll_next() has been called and last returned poll::ready.
    95                  virtual auto value() const -> const output_type& = 0;
    96          };
    97
    98          // Specialization for `void` output type.
    99          template<>
   100          struct producer<void> : node {
   101                  using output_type = void;
   102          };
   103
   104          template<typename Input, typename Output>
   105          struct component : producer<Output> {
   106                  using input_type = Input;
   107                  using output_type = Output;
   108          };
   109
   110          template<typename Input>
   111          struct sink : component<std::tuple<Input>, void> {};
   112
   113          template<typename Output>
   114          struct source : component<std::tuple<>, Output> {
   115           private:
   116                  void connect(const node* source, int slot) override {
   117                          (void)source;
   118                          (void)slot;
   119                  }
   120          };
   121
   122          // The requirements that a type `N` must satisfy
   123          // to be used as a component in a pipeline.
   124          template<typename N>
   125          // 3.6.0
   126          concept concrete_node =
   127              requires {
   128                      typename N::input_type;
   129                      typename N::output_type;
   130                      std::tuple_size_v<typename N::input_type>;
   131              } and std::derived_from<N, node> and std::derived_from<N, producer<typename N::output_type>> and !
   132          std::is_abstract_v<N>;
   133
   134          namespace internal {
   135                  enum class node_type { source, sink, component };
   136          }
   137
   138          class pipeline {
   139           public:
   140                  // 3.6.1
   141                  using node_id = std::size_t;
   142
   143                  // 3.6.2
   144                  pipeline() noexcept
   145                  : id_counter_(0) {}
   146                  pipeline(const pipeline&) = delete;
   147                  pipeline(pipeline&& other) noexcept
   148                  : id_counter_(other.id_counter_)
   149                  , nodes_(std::move(other.nodes_))
   150                  , dependencies_(std::move(other.dependencies_)) {}
   151                  auto operator=(const pipeline&) -> pipeline& = delete;
   152                  auto operator=(pipeline&& other) noexcept -> pipeline& {
   153                          id_counter_ = other.id_counter_;
   154                          nodes_ = std::move(other.nodes_);
   155                          dependencies_ = std::move(other.dependencies_);
   156                          return *this;
   157                  }
   158                  ~pipeline() noexcept {
   159                          nodes_.clear();
   160                          dependencies_.clear();
   161                  }
   162
   163                  // 3.6.3
   164                  template<typename N, typename... Args>
   165                      requires concrete_node<N>
   166                               and std::constructible_from<N, Args...>
   167                               auto create_node(Args&&... args) -> node_id {
   168                          auto id = ++id_counter_;
   169                          using input_type = typename N::input_type;
   170                          using output_type = typename N::output_type;
   171
   172                          auto node_input_types = std::vector<std::type_index>{};
   173                          std::apply(
   174                              [&node_input_types](auto&&... types) {
   175                                      (node_input_types.emplace_back(typeid(std::decay_t<decltype(types)>)), ...);
   176                              },
   177                              input_type{});
   178
   179                          internal::node_type saved_n_type = internal::node_type::component;
   180                          if constexpr (std::is_base_of_v<source<typename N::output_type>, N>) {
   181                                  saved_n_type = internal::node_type::source;
   182                          }
   183                          else if constexpr (std::tuple_size_v<input_type> == 1) {
   184                                  if (std::is_base_of_v<sink<std::tuple_element_t<0, input_type>>, N>) {
   185                                          saved_n_type = internal::node_type::sink;
   186                                  }
   187                          }
   188
   189                          dependencies_.emplace(id, std::vector<std::pair<node_id, int>>{});
   190                          nodes_.emplace(id,
   191                                         std::make_tuple(std::make_unique<N>(std::forward<Args>(args)...),
   192                                                         std::move(node_input_types),
   193                                                         std::type_index(typeid(output_type)),
   194                                                         saved_n_type));
   195                          return id;
   196                  }
   197
   198                  // Remove the specified node from the pipeline. Disconnects it from any nodes it is currently connected to.
   199                  void erase_node(node_id n_id) {
   200                          throw_if_id_invalid(n_id);
   201                          for (auto& node : dependencies_) {
   202                                  node.second.erase(std::remove_if(node.second.begin(),
   203                                                                   node.second.end(),
   204                                                                   [n_id](const auto& src) { return src.first == n_id; }),
   205                                                    node.second.end());
   206                          }
   207                          dependencies_.erase(n_id);
   208                          nodes_.erase(n_id);
   209                  }
   210
   211                  // Returns: A pointer to the specified node. If node is invalid, returns nullptr instead.
   212                  auto get_node(node_id n_id) noexcept -> node* {
   213                          if (nodes_.find(n_id) == nodes_.end()) {
   214                                  return nullptr;
   215                          }
   216                          return std::get<node_ptr>(nodes_.at(n_id)).get();
   217                  }
   218
   219                  // Notes: You may need more than one overload for proper const-correctness.
   220                  auto get_node(node_id n_id) const noexcept -> const node* {
   221                          if (nodes_.find(n_id) == nodes_.end()) {
   222                                  return nullptr;
   223                          }
   224                          return std::get<node_ptr>(nodes_.at(n_id)).get();
   225                  }
   226
   227                  // 3.6.4
   228                  // Connect source's output to dest's input for the given slot.
   229                  void connect(node_id src, node_id dst, int slot) {
   230                          //? Check if either handle is invalid
   231                          throw_if_id_invalid(src);
   232                          throw_if_id_invalid(dst);
   233
   234                          //? Check if the destination node's slot is already full
   235                          for (auto& node : dependencies_) {
   236                                  for (auto& curr_src : node.second) {
   237                                          if (curr_src.first == dst && curr_src.second == slot) {
   238                                                  throw pipeline_error(pipeline_error_kind::slot_already_used);
   239                                          }
   240                                  }
   241                          }
   242
   243                          //? Check if the slot number indicated by slot does not exist
   244                          if (slot < 0 || slot >= get_input_size(dst)) {
   245                                  throw pipeline_error(pipeline_error_kind::no_such_slot);
   246                          }
   247
   248                          //? Check if the source output type does not match the destination input type on corresponding slot
   249                          if (get_output_type(src) != get_input_type(dst, slot)) {
   250                                  throw pipeline_error(pipeline_error_kind::connection_type_mismatch);
   251                          }
   252
   253                          //? Update the dependencies_ map
   254                          dependencies_.at(src).emplace_back(dst, slot);
   255
   256                          //? Connect the nodes
   257                          if (get_node(src) != nullptr && get_node(dst) != nullptr) {
   258                                  get_node(dst)->connect(get_node(src), slot);
   259                          }
   260                  }
   261
   262                  void disconnect(node_id src, node_id dst) {
   263                          //? Check if either handle is invalid
   264                          throw_if_id_invalid(src);
   265                          throw_if_id_invalid(dst);
   266                          if (dependencies_.at(src).empty()) {
   267                                  return;
   268                          }
   269                          auto& src_dep = dependencies_.at(src);
   270                          src_dep.erase(
   271                              std::remove_if(src_dep.begin(), src_dep.end(), [dst](const auto& curr) { return curr.first == dst; }),
   272                              src_dep.end());
   273                  }
   274
   275                  auto get_dependencies(node_id src) const -> std::vector<std::pair<node_id, int>> {
   276                          throw_if_id_invalid(src);
   277                          return dependencies_.at(src);
   278                  }
   279
   280                  // 3.6.5
   281                  auto is_valid() const -> bool {
   282                          //? All source slots for all nodes must be filled.
   283                          int count = 0;
   284                          for (auto& node : nodes_) {
   285                                  count += get_input_size(node.first);
   286                          }
   287                          for (auto& node : dependencies_) {
   288                                  count -= static_cast<int>(node.second.size());
   289                          }
   290                          if (count != 0) {
   291                                  return false;
   292                          }
   293
   294                          //? There is at least 1 source node.
   295                          count = 0;
   296                          for (auto& node : nodes_) {
   297                                  if (get_node_type(node.first) == internal::node_type::source) {
   298                                          count++;
   299                                  }
   300                          }
   301                          if (count == 0) {
   302                                  return false;
   303                          }
   304
   305                          //? There is at least 1 sink node.
   306                          count = 0;
   307                          for (auto& node : nodes_) {
   308                                  if (get_node_type(node.first) == internal::node_type::sink) {
   309                                          count++;
   310                                  }
   311                          }
   312                          if (count == 0) {
   313                                  return false;
   314                          }
   315
   316                          //? All non-sink nodes must have at least one dependent.
   317                          for (auto& node : nodes_) {
   318                                  if (dependencies_.at(node.first).empty() && get_node_type(node.first) != internal::node_type::sink) {
   319                                          return false;
   320                                  }
   321                          }
   322
   323                          // //? There are no subpipelines.
   324                          std::vector<node_id> visited{};
   325                          std::stack<node_id> sub_stack{};
   326
   327                          sub_stack.push(nodes_.begin()->first);
   328
   329                          while (!sub_stack.empty()) {
   330                                  auto curr = sub_stack.top();
   331                                  sub_stack.pop();
   332                                  if (std::find(visited.begin(), visited.end(), curr) != visited.end()) {
   333                                          continue;
   334                                  }
   335                                  visited.push_back(curr);
   336                                  // add all dependencies of curr to the stack
   337                                  for (auto& node : dependencies_) {
   338                                          if (node.first == curr) {
   339                                                  for (auto& src : node.second) {
   340                                                          if (std::find(visited.begin(), visited.end(), src.first) == visited.end()) {
   341                                                                  sub_stack.push(src.first);
   342                                                          }
   343                                                  }
   344                                          }
   345                                  }
   346                                  // add all dependent of curr to the stack
   347                                  for (auto& node : dependencies_) {
   348                                          for (auto& src : node.second) {
   349                                                  if (src.first == curr) {
   350                                                          if (std::find(visited.begin(), visited.end(), node.first) == visited.end()) {
   351                                                                  sub_stack.push(node.first);
   352                                                          }
   353                                                  }
   354                                          }
   355                                  }
   356                          }
   357
   358                          if (visited.size() != nodes_.size()) {
   359                                  return false;
   360                          }
   361
   362                          //? There are no cycles. Use Kahn's algorithm
   363                          std::map<node_id, int> in_degree{};
   364                          std::map<node_id, std::vector<node_id>> adj_list{};
   365                          std::queue<node_id> q{};
   366
   367                          // Initialize in_degree and adj_list
   368                          for (auto& node : nodes_) {
   369                                  auto n_id = node.first;
   370                                  in_degree[n_id] = 0;
   371                                  adj_list[n_id] = std::vector<node_id>{};
   372                          }
   373
   374                          // Update in_degree and adj_list
   375                          for (auto& node : nodes_) {
   376                                  auto n_id = node.first;
   377                                  auto node_inputs = dependencies_.at(n_id);
   378                                  for (auto& src : node_inputs) {
   379                                          in_degree[src.first]++;
   380                                          adj_list[n_id].push_back(src.first);
   381                                  }
   382                          }
   383
   384                          // Push all nodes with in_degree == 0 into the queue
   385                          for (auto& it : in_degree) {
   386                                  if (it.second == 0) {
   387                                          q.push(it.first);
   388                                  }
   389                          }
   390
   391                          // Pop nodes from the queue and update in_degree and adj_list
   392                          while (!q.empty()) {
   393                                  auto curr = q.front();
   394                                  q.pop();
   395                                  for (auto& next : adj_list[curr]) {
   396                                          in_degree[next]--;
   397                                          if (in_degree[next] == 0) {
   398                                                  q.push(next);
   399                                          }
   400                                  }
   401                          }
   402
   403                          // Check if there is a node with in_degree > 0
   404                          for (auto& it : in_degree) {
   405                                  if (it.second > 0) {
   406                                          return false;
   407                                  }
   408                          }
                                ^
+ ========================================================================= +
+ -0.5: Not using a std::algorithm function when one is clearly appropriate +
+ Prefer `std::any_of` here                                                 +
+ ========================================================================= +
   409
   410                          return true;
   411                  }
                        ^
+ ============================================================================== +
+ -0.5: Not separating large functions in appropriately named smaller functions. +
+ ============================================================================== +
   412
   413                  // Perform one tick of the pipeline.
   414                  // Initially source nodes shall be polled, and will prepare a value.
   415                  // According to the poll result:
   416                  // - If the node is closed, close all nodes that depend on it.
   417                  // - If the node has no value, skip all nodes that depend on it.
   418                  // - Otherwise, the node has a value, and all nodes that depend on it should be polled, and so on recursively.
   419                  // The tick ends once every node has been either polled, skipped, or closed.
   420                  auto step() -> bool {
   421                          auto flow = std::vector<node_id>{};
   422                          for (auto& node : nodes_) {
   423                                  auto curr_id = node.first;
   424                                  auto curr = get_node(curr_id);
   425                                  if (get_node_type(curr_id) == internal::node_type::source && curr->get_state() == poll::ready) {
   426                                          flow.emplace_back(curr_id);
   427                                  }
   428                          }
   429
   430                          while (!flow.empty()) {
   431                                  auto curr_id = flow.front();
   432                                  flow.erase(flow.begin());
   433                                  auto curr = get_node(curr_id);
   434
   435                                  // avoid polling a node if all its dependent sink nodes are closed.
   436                                  bool all_closed = true;
   437                                  if (get_node_type(curr_id) != internal::node_type::sink) {
   438                                          for (auto& dst : dependencies_.at(curr_id)) {
   439                                                  if (get_node(dst.first)->get_state() != poll::closed) {
   440                                                          all_closed = false;
   441                                                          break;
   442                                                  }
   443                                          }
   444                                          if (all_closed) {
   445                                                  continue;
   446                                          }
   447                                  }
   448
   449                                  // polling a node only if all its dependencies are ready.
   450                                  bool dependencies_check = false;
   451                                  for (auto& node : dependencies_) {
   452                                          for (auto& dst : node.second) {
   453                                                  if (dst.first == curr_id && get_node(node.first)->get_state() != poll::ready) {
   454                                                          dependencies_check = true;
   455                                                          break;
   456                                                  }
   457                                          }
   458                                  }
   459                                  if (dependencies_check) {
   460                                          continue;
   461                                  }
   462
   463                                  auto poll_state = curr->poll_next();
   464                                  curr->set_state(poll_state);
   465                                  switch (poll_state) {
   466                                  case poll::closed: recursively_close(curr_id); break;
   467                                  case poll::empty: break;
   468                                  case poll::ready:
   469                                          for (auto& dst : dependencies_.at(curr_id)) {
   470                                                  if (get_node(dst.first)->get_state() == poll::closed
   471                                                      || std::find(flow.begin(), flow.end(), dst.first) != flow.end())
   472                                                  {
   473                                                          continue;
   474                                                  }
   475                                                  flow.emplace_back(dst.first);
   476                                          }
   477                                          break;
   478                                  }
   479                          }
   480
   481                          // Returns: true if all sink nodes are now closed, or false otherwise.
   482                          for (auto& node : nodes_) {
   483                                  auto curr_id = node.first;
   484                                  auto curr = get_node(curr_id);
   485                                  if (get_node_type(curr_id) == internal::node_type::sink && curr->get_state() != poll::closed) {
   486                                          return false;
   487                                  }
   488                          }
   489
   490                          return true;
   491                  }
   492
   493                  void run() {
   494                          if (!is_valid()) {
   495                                  return;
   496                          }
   497                          while (!step()) {
   498                          }
   499                  }
   500
   501                  // 3.6.6
   502                  friend std::ostream& operator<<(std::ostream& os, const pipeline& pip) {
   503                          auto format_node = [&pip](node_id n_id) {
   504                                  std::string n_name{};
   505                                  if (pip.get_node(n_id) != nullptr) {
   506                                          n_name = pip.get_node(n_id)->name();
   507                                  }
   508                                  else {
   509                                          n_name = "unnamed";
   510                                  }
   511                                  return "\"" + std::to_string(n_id) + " " + n_name + "\"";
   512                          };
   513
   514                          os << "digraph G {" << std::endl;
   515                          for (auto& node : pip.nodes_) {
   516                                  auto n_id = node.first;
   517                                  os << "  " << format_node(n_id) << std::endl;
   518                          }
   519
   520                          os << std::endl;
   521
   522                          for (auto& node : pip.nodes_) {
   523                                  auto n_id = node.first;
   524                                  auto node_outputs = pip.dependencies_.at(n_id);
   525                                  for (auto& dst : node_outputs) {
   526                                          os << "  " << format_node(n_id) << " -> " << format_node(dst.first) << std::endl;
   527                                  }
   528                          }
   529
   530                          os << "}" << std::endl;
   531
   532                          return os;
   533                  }
   534
   535           private:
   536                  node_id id_counter_;
   537                  std::map<node_id, std::tuple<std::unique_ptr<node>, std::vector<std::type_index>, std::type_index, internal::node_type>>
   538                      nodes_{};
   539                  std::map<node_id, std::vector<std::pair<node_id, int>>> dependencies_{};
   540
   541                  static const std::size_t node_ptr = 0;
   542                  static const std::size_t input_types = 1;
   543                  static const std::size_t output_type = 2;
   544                  static const std::size_t n_type = 3;
   545
   546                  // Throws: a pipeline_error for an invalid node ID.
   547                  void throw_if_id_invalid(node_id n_id) const {
   548                          if (nodes_.find(n_id) == nodes_.end()) {
   549                                  throw pipeline_error(pipeline_error_kind::invalid_node_id);
   550                          }
   551                  }
   552
   553                  auto get_input_size(node_id n_id) const noexcept -> int {
   554                          return static_cast<int>(std::get<input_types>(nodes_.at(n_id)).size());
   555                  }
   556
   557                  auto get_input_type(node_id n_id, int slot) const noexcept -> std::type_index {
   558                          return std::get<input_types>(nodes_.at(n_id)).at(static_cast<std::size_t>(slot));
   559                  }
   560
   561                  auto get_output_type(node_id n_id) const noexcept -> std::type_index {
   562                          return std::get<output_type>(nodes_.at(n_id));
   563                  }
   564
   565                  auto get_node_type(node_id n_id) const noexcept -> internal::node_type {
   566                          return std::get<n_type>(nodes_.at(n_id));
   567                  }
   568
   569                  void recursively_close(node_id id) noexcept {
   570                          auto curr = get_node(id);
   571                          curr->set_state(poll::closed);
   572                          for (auto& dst : dependencies_.at(id)) {
   573                                  recursively_close(dst.first);
   574                          }
   575                  }
   576          };
   577
   578  } // namespace ppl
   579
   580  #endif // COMP6771_PIPELINE_H

