   331  TEST_CASE("connect - successful connection") {
   332          auto pip = ppl::pipeline{};
   333          const auto source_a = pip.create_node<source_produce_num<int, 0, 10>>();
   334          const auto source_b = pip.create_node<source_produce_num<int, 10, 0>>();
   335          const auto component = pip.create_node<component_add<int, int, int>>();
   336          const auto sink = pip.create_node<sink_print<int>>();
   337
   338          pip.connect(source_a, component, 0);
   339          pip.connect(source_b, component, 1);
   340          pip.connect(component, sink, 0);
   341
   342          auto res = pip.get_dependencies(source_a);
   343          CHECK(res.size() == 1);
   344          CHECK(res[0] == std::make_pair(component, 0));
   345
   346          auto res2 = pip.get_dependencies(source_b);
   347          CHECK(res2.size() == 1);
   348          CHECK(res2[0] == std::make_pair(component, 1));
   349
   350          auto res3 = pip.get_dependencies(component);
   351          CHECK(res3.size() == 1);
   352          CHECK(res3[0] == std::make_pair(sink, 0));
   353  }
