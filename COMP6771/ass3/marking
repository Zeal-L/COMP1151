   311          auto pip = ppl::pipeline{};
   312          pip.create_node<source_produce_num<int, 0, 10>>();
   313
   314          CHECK_THROWS_MATCHES(pip.erase_node(9), ppl::pipeline_error, Catch::Matchers::Message("invalid node ID"));
   315  }
