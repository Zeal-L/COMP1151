     9  // dot -T svg client.dot -o client.svg
    10  // dot -T png client.dot -o client.png
    11
    12  template<typename T, T from, T to>
    13      requires std::is_arithmetic_v<T>
    14  struct source_produce_num : ppl::source<T> {
    15          T current_value;
    16
    17          source_produce_num()
    18          : current_value(from) {}
    19
    20          auto name() const -> std::string override {
    21                  return std::string("Source - produce ") + typeid(T).name() + ": " + std::to_string(from) + " to "
    22                         + std::to_string(to);
    23          }
    24
    25          auto poll_next() -> ppl::poll override {
    26                  if (from < to) {
    27                          if (current_value > to)
    28                                  return ppl::poll::closed;
    29                          ++current_value;
    30                          return ppl::poll::ready;
    31                  }
    32                  else if (from > to) {
    33                          if (current_value < to)
    34                                  return ppl::poll::closed;
    35                          --current_value;
    36                          return ppl::poll::ready;
    37                  }
    38                  else {
    39                          return ppl::poll::closed;
    40                  }
    41          }
    42
    43          auto value() const -> const T& override {
    44                  return current_value;
    45          }
    46  };
    47
    48  template<typename T>
    49  concept printable = requires(std::ostream& os, const T& t) { os << t; };
    50
    51  template<typename T>
    52      requires printable<T>
    53  struct sink_print : ppl::sink<T> {
    54          const ppl::producer<T>* slot0 = nullptr;
    55
    56          sink_print() = default;
    57
    58          auto name() const -> std::string override {
    59                  return "Sink - print";
    60          }
    61
    62          void connect(const ppl::node* src, int slot) override {
    63                  if (slot == 0) {
    64                          slot0 = static_cast<const ppl::producer<T>*>(src);
    65                  }
    66          }
    67
    68          auto poll_next() -> ppl::poll override {
    69                  std::cout << slot0->value() << '\n';
    70                  return ppl::poll::ready;
    71          }
    72  };
    73
    74  template<typename T, typename U, typename O>
    75      requires std::is_arithmetic_v<T> && std::is_arithmetic_v<U> && std::is_arithmetic_v<O>
    76  struct component_add : ppl::component<std::tuple<T, U>, O> {
    77          const ppl::producer<T>* slot0 = nullptr;
    78          const ppl::producer<U>* slot1 = nullptr;
    79
    80          mutable O result{};
    81
    82          component_add() = default;
    83
    84          auto name() const -> std::string override {
    85                  return std::string("Component - add ") + typeid(T).name() + " " + typeid(U).name();
    86          }
    87
    88          void connect(const ppl::node* src, int slot) override {
    89                  if (slot == 0) {
    90                          slot0 = static_cast<const ppl::producer<T>*>(src);
    91                  }
    92                  else if (slot == 1) {
    93                          slot1 = static_cast<const ppl::producer<U>*>(src);
    94                  }
    95          }
    96
    97          auto poll_next() -> ppl::poll override {
    98                  return ppl::poll::ready;
    99          }
   100
   101          auto value() const -> const O& override {
   102                  result = static_cast<const O&>(slot0->value()) + static_cast<const O&>(slot1->value());
   103                  return result;
   104          }
   105  };
   106
   107  template<typename T, typename U, typename O>
   108      requires std::is_arithmetic_v<T> && std::is_arithmetic_v<U> && std::is_arithmetic_v<O>
   109  struct component_add_but_skip_odd : ppl::component<std::tuple<T, U>, O> {
   110          const ppl::producer<T>* slot0 = nullptr;
   111          const ppl::producer<U>* slot1 = nullptr;
   112
   113          mutable O result{};
   114
   115          component_add_but_skip_odd() = default;
   116
   117          auto name() const -> std::string override {
   118                  return std::string("Component - add but skip odd ") + typeid(T).name() + " " + typeid(U).name();
   119          }
   120
   121          void connect(const ppl::node* src, int slot) override {
   122                  if (slot == 0) {
   123                          slot0 = static_cast<const ppl::producer<T>*>(src);
   124                  }
   125                  else if (slot == 1) {
   126                          slot1 = static_cast<const ppl::producer<U>*>(src);
   127                  }
   128          }
   129
   130          auto poll_next() -> ppl::poll override {
   131                  if (slot0->value() % 2 == 0 && slot1->value() % 2 == 0) {
   132                          return ppl::poll::ready;
   133                  }
   134                  else {
   135                          return ppl::poll::empty;
   136                  }
   137          }
   138
   139          auto value() const -> const O& override {
   140                  result = static_cast<const O&>(slot0->value()) + static_cast<const O&>(slot1->value());
   141                  return result;
   142          }
   143  };
   144
   145  template<typename T>
   146      requires std::is_arithmetic_v<T>
   147  struct component_double_it : ppl::component<std::tuple<T>, T> {
   148          const ppl::producer<T>* slot0 = nullptr;
   149
   150          component_double_it() = default;
   151
   152          mutable T result{};
   153
   154          auto name() const -> std::string override {
   155                  return std::string("Component - double it ") + typeid(T).name();
   156          }
   157
   158          void connect(const ppl::node* src, int slot) override {
   159                  if (slot == 0) {
   160                          slot0 = static_cast<const ppl::producer<T>*>(src);
   161                  }
   162          }
   163
   164          auto poll_next() -> ppl::poll override {
   165                  return ppl::poll::ready;
   166          }
   167
   168          auto value() const -> const T& override {
   169                  result = slot0->value() * 2;
   170                  return result;
   171          }
   172  };
   173
   174  template<typename T>
   175      requires std::is_arithmetic_v<T>
   176  struct component_double_it_but_skip_odd : ppl::component<std::tuple<T>, T> {
   177          const ppl::producer<T>* slot0 = nullptr;
   178
   179          component_double_it_but_skip_odd() = default;
   180
   181          mutable T result{};
   182
   183          auto name() const -> std::string override {
   184                  return std::string("Component - double it but skip odd ") + typeid(T).name();
   185          }
   186
   187          void connect(const ppl::node* src, int slot) override {
   188                  if (slot == 0) {
   189                          slot0 = static_cast<const ppl::producer<T>*>(src);
   190                  }
   191          }
   192
   193          auto poll_next() -> ppl::poll override {
   194                  if (slot0->value() % 2 == 0) {
   195                          return ppl::poll::ready;
   196                  }
   197                  else {
   198                          return ppl::poll::empty;
   199                  }
   200          }
   201
   202          auto value() const -> const T& override {
   203                  result = slot0->value() * 2;
   204                  return result;
   205          }
