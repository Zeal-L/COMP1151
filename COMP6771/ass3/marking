   758  //              Validation and Execution
   759  // ########################################
   760
   761  TEST_CASE("std::ostream &operator<<") {
   762          auto pip = ppl::pipeline{};
   763          const auto source1 = pip.create_node<source_produce_num<int, 0, 10>>();
   764          const auto source2 = pip.create_node<source_produce_num<int, 0, 10>>();
   765          const auto component = pip.create_node<component_add<int, int, int>>();
   766          const auto sink = pip.create_node<sink_print<int>>();
   767
   768          pip.connect(source1, component, 0);
   769          pip.connect(source2, component, 1);
   770          pip.connect(component, sink, 0);
   771
   772          CHECK(pip.is_valid());
   773
   774          std::stringstream buffer;
   775
   776          buffer << pip;
   777
   778          auto expect = R"(digraph G {
   779    "1 Source - produce i: 0 to 10"
   780    "2 Source - produce i: 0 to 10"
   781    "3 Component - add i i"
   782    "4 Sink - print"
   783
   784    "1 Source - produce i: 0 to 10" -> "3 Component - add i i"
   785    "2 Source - produce i: 0 to 10" -> "3 Component - add i i"
   786    "3 Component - add i i" -> "4 Sink - print"
   787  }
   788  )";
   789
   790          CHECK(buffer.str() == expect);
   791  }
        ^
