   212  TEST_CASE("Move Constructor") {
   213          std::string line;
   214          std::stringstream buffer;
   215          std::streambuf* old = std::cout.rdbuf(buffer.rdbuf());
   216
   217          auto pip = ppl::pipeline{};
   218          const auto source = pip.create_node<source_produce_num<int, 0, 10>>();
   219          const auto sink = pip.create_node<sink_print<int>>();
   220
   221          pip.connect(source, sink, 0);
   222          pip.step();
   223          pip.step();
   224          auto pip2 = std::move(pip);
   225          CHECK(pip.get_node(source) == nullptr);
   226          CHECK(pip2.get_node(source) != nullptr);
   227          pip2.run();
   228
   229          int count = 0;
   230          while (std::getline(buffer, line)) {
   231                  CHECK(line == std::to_string(++count));
   232          }
   233
   234          std::cout.rdbuf(old);
   235  }
   236
   237  TEST_CASE("Move Assignment") {
   238          std::string line;
   239          std::stringstream buffer;
   240          std::streambuf* old = std::cout.rdbuf(buffer.rdbuf());
   241
   242          auto pip = ppl::pipeline{};
   243          const auto source = pip.create_node<source_produce_num<int, 0, 10>>();
   244          const auto sink = pip.create_node<sink_print<int>>();
   245
   246          pip.connect(source, sink, 0);
   247          pip.step();
   248          pip.step();
   249          ppl::pipeline pip2{};
   250          pip2 = std::move(pip);
   251          pip2.run();
   252
   253          int count = 0;
   254          while (std::getline(buffer, line)) {
   255                  CHECK(line == std::to_string(++count));
   256          }
   257
   258          std::cout.rdbuf(old);
   259  }
